{
    "name": "dframe/database",
    "description": "Database pdo wrapper",
    "type": "library",
    "keywords": [
        "database",
        "mysql",
        "pdo",
        "pdowraper",
        "wraper",
        "update",
        "query-builder",
        "query"
    ],
    "homepage": "https://dframeframework.com",
    "license": "MIT",
    "authors": [
        {
            "name": "SÅ‚awomir Kaleta",
            "email": "slaszka@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Dframe\\Database\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dframe\\Database\\Tests\\": "tests/"
        }
    },
    "require": {
        "php": ">=5.6",
        "ext-pdo": "*",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7 || ^6.5",
        "phpstan/phpstan": "^0.11"
    }
}
